msgid "event-leave-normal"
msgstr "${userName} verlassen (${date})"

#. Default: "${userName} removed by ${adminName} (${date})"
#: ./monthlog.py:120
msgid "event-leave-other"
msgstr "${userName} entfernt von ${adminName} (${date})"

#. Default: "${n} members:"
#: ./browser/templates/detail.pt:60 ./browser/templates/summary.pt:12
msgid "summary-all-members-n"
msgstr "${n} Mitglieder:"

#. Default: "One member:"
#: ./browser/templates/detail.pt:56 ./browser/templates/summary.pt:8
msgid "summary-all-members-one"
msgstr "Ein Mitglied:"

#. Default: "${n} joined,"
#: ./browser/templates/detail.pt:63 ./browser/templates/summary.pt:16
msgid "summary-joined"
msgstr "${n} beigetreten,"

#. Default: "${n} left."
