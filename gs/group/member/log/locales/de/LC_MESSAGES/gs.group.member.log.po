#. Default: "This detailed information is only visible to administrators.
#. Normal members just see how many people joined and left each month."
#: ./browser/templates/detail.pt:5
msgid "detail-intro"
msgstr "Die detaillierten Informationen sind nur für Administratoren zugänglich. Normale Mitglieder können lediglich sehen, wieviele Nutzer monatlich beitreten und austreten. "

#. Default: "No join or leave events to display."
#: ./browser/templates/detail.pt:99
msgid "detail-none"
msgstr "Keine Betritte und Austritte zum Anzeigen verfügbar."

#. Default: "Show ${arrow}"
#: ./browser/templates/detail.pt:12
msgid "detail-show-button"
msgstr "Anzeigen ${arrow}"

#. Default: "${tick}Show all"
#: ./browser/templates/detail.pt:20
msgid "detail-show-button-all"
msgstr "${tick}Alle anzeigen"

#. Default: "${tick}Show just joining"
#: ./browser/templates/detail.pt:28
msgid "detail-show-button-join"
msgstr "${tick}Zeige nur beigetretene Mitglieder"

#. Default: "${tick}Show just leaving"
#: ./browser/templates/detail.pt:36
msgid "detail-show-button-leave"
msgstr "${tick}Zeige nur ausgetretene Mitglieder"

#. Default: "Detailed log"
#: ./viewlet.py:44
msgid "detail-title"
msgstr "Detailliertes Protokoll"

#. Default: "${userName} joined &#8212; invited by ${adminName} (${date})"
#: ./monthlog.py:94
msgid "event-join-admin"
msgstr "${userName} beigetreten &#8212; eingeladen von ${adminName} (${date})"

#. Default: "${userName} joined (${date})"
#: ./monthlog.py:100
msgid "event-join-normal"
msgstr "${userName} beigetreten (${date})"

#. Default: "${userName} left (${date})"
